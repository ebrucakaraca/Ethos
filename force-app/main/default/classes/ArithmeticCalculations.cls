public class ArithmeticCalculations {
    public integer add(integer x, integer y){
    integer z = x + y;
    return z;
}    

    public integer substract(integer x, integer y){
    integer z = x - y;
    return z;
}
    public integer multiply(integer x, integer y, integer m){
    integer z = x * y * m;
    return z;
}


}
